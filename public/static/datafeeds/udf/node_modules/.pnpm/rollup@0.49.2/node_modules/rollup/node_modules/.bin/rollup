#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/safouene/Guacamole-official/public/static/datafeeds/udf/node_modules/.pnpm/rollup@0.49.2/node_modules/rollup/bin/node_modules:/home/safouene/Guacamole-official/public/static/datafeeds/udf/node_modules/.pnpm/rollup@0.49.2/node_modules/rollup/node_modules:/home/safouene/Guacamole-official/public/static/datafeeds/udf/node_modules/.pnpm/rollup@0.49.2/node_modules:/home/safouene/Guacamole-official/public/static/datafeeds/udf/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/safouene/Guacamole-official/public/static/datafeeds/udf/node_modules/.pnpm/rollup@0.49.2/node_modules/rollup/bin/node_modules:/home/safouene/Guacamole-official/public/static/datafeeds/udf/node_modules/.pnpm/rollup@0.49.2/node_modules/rollup/node_modules:/home/safouene/Guacamole-official/public/static/datafeeds/udf/node_modules/.pnpm/rollup@0.49.2/node_modules:/home/safouene/Guacamole-official/public/static/datafeeds/udf/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/rollup" "$@"
else
  exec node  "$basedir/../../bin/rollup" "$@"
fi
